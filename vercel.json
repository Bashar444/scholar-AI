  },
  "headers": [
    {
      "source": "/api/(.*)",
      "headers": [
        {
          "key": "Cache-Control",
          "value": "no-cache, no-store, must-revalidate"
        }
      ]
    }
  ],
  "rewrites": [
    {
      "source": "/api/(.*)",
      "destination": "https://your-backend-api.com/api/$1"
    }
  ],
  "redirects": [
    {
      "source": "/docs",
      "destination": "/api",
      "permanent": false
    }
  ],
  "cleanUrls": true,
  "trailingSlash": false,
  "regions": ["iad1"],
  "functions": {
    "frontend/pages/api/**": {
      "maxDuration": 60,
      "memory": 1024
    }
  },
  "git": {
    "deploymentEnabled": {
      "main": true
    }
  }
}
